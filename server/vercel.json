{
    "version": 2,
    "builds": [
        {
        "src": "node_server/index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": "node_server/src/**,python_scripts/**" 
        }
        }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "node_server/index.js" }
    ]
}